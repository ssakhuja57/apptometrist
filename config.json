{
    "hostname": "%(HOST_NAME)s",
    "update-freq": 10,
    "routine-file": "/etc/devops/monitor.json",
    "server": {
        "address": "%(SERVER_ADDR)s",
        "port": 4567,
        "secure": false
    },
    "frequencies": [
            {
                "pattern": "monitor-agent",
                "seconds": null
            },
            {
                "pattern": ".*sensu.*", 
                "seconds": null 
            },
            {
                "pattern": ".*",
                "seconds": 10 
            }
    ]
}
